{
  "name": "qr-displayer-poap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@poap/poap-components": "^0.1.9",
    "@rive-app/react-canvas": "^3.0.53",
    "@vitejs/plugin-react": "^4.0.4",
    "cypress": "^11.2.0",
    "firebase": "^10.3.1",
    "formik": "^2.4.4",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-recaptcha": "^2.1.0",
    "react-i18next": "^12.1.4",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.15.0",
    "sass": "^1.66.1",
    "styled-components": "^5.3.6",
    "use-interval": "^1.4.0",
    "uuid": "^9.0.0",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "vite --host",
    "build": "vite build",
    "cypress": "NODE_ENV=development cypress open --e2e --browser chrome",
    "test": "CYPRESS_LOCAL=true NODE_ENV=development start-server-and-test --expect 200 'BROWSER=none npm start' 3000 \"cypress run --browser chrome\"",
    "test:production": "npm run build && NODE_ENV=production start-server-and-test --expect 200 'serve build' 3000 \"cypress open --browser chrome\"",
    "test:ci": "NODE_ENV=production CI=true start-server-and-test --expect 200 start 3000 \"cypress run --browser chrome --config 60000\"",
    "lint": "eslint --fix src ",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@poap/skunk-linter": "^0.0.3",
    "core-js": "^3.32.2",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.3.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "serve": "^14.2.1",
    "start-server-and-test": "^1.15.2"
  }
}